{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "Booking",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "HowManyPeople": {
      "type": "integer"
    },
    "TotalPrice": {
      "type": "decimal"
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "bookings"
    },
    "package": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::package.package",
      "inversedBy": "bookings"
    },
    "slip": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "Status_booking": {
      "type": "enumeration",
      "enum": [
        "pending",
        "approved",
        "rejected",
        "completed"
      ],
      "default": "pending",
      "required": true
    },
    "Start": {
      "type": "date",
      "required": true
    },
    "End": {
      "type": "date"
    },
    "RejectionReason": {
      "type": "text"
    }
  }
}
