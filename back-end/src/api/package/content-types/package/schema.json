{
  "kind": "collectionType",
  "collectionName": "packages",
  "info": {
    "singularName": "package",
    "pluralName": "packages",
    "displayName": "Package",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "One day trip",
        "Muti day trip"
      ],
      "required": true
    },
    "Description": {
      "type": "blocks"
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "package"
    },
    "Date": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::travel-date.travel-date",
      "mappedBy": "package"
    },
    "Price": {
      "type": "decimal",
      "required": true
    },
    "MeetingPoint": {
      "type": "string",
      "required": true
    }
  }
}
